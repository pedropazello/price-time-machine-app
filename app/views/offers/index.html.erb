<h1>
    Offers:
</h1>
<br/>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Price</th>
      <th>URL</th>
    </tr>
  </thead>
  <tbody>
    <% @offers.each do |offer| %>
      <tr>
        <td><%= offer.id %></td>
        <td><%= offer.name %></td>
        <td><%= offer.price %></td>
        <td><%= link_to("url", offer.url) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
